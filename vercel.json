{
  "name": "API-Sendmail",
  "version": "1.0.0",
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "dust/send-email.js",
      "dest": "dist/routes/email.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ],
  "dependencies": {
    "express": "^4.17.1",
    "@types/express": "^4.17.11",
    "typescript": "^4.4.4",
    "@sendgrid/mail": "^7.7.0"
  }
}
