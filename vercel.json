{
  "name": "API-Sendmail",
  "version": 1,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "dust/send-email.js",
      "dest": "dist/routes/email.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ]
}
